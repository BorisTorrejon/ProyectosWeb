@model ENTIDAD.Proyecto

@{
    ViewBag.Title = "Detalle de Proyecto";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h4>Detalle de Proyecto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Nombre de Proyecto:
        </dt>

        <dd>
            @Model.NombreProyecto
        </dd>

        <dt>
            Fecha de Inicio:
        </dt>

        <dd>
            @Model.FechaInicio.ToShortDateString()
        </dd>

        <dt>
            Fecha Fin:
        </dt>

        <dd>
            @Model.FechaFin.ToShortDateString()
        </dd>

    </dl>
</div>
<p>
    <a class="btn btn-primary" href="@Url.Action("Index","Proyecto")">Regresar</a>
    <a class="btn btn-primary" href="@Url.Action("Editar","Proyecto",new {id=Model.ProyectoID})">Editar</a>
</p>
